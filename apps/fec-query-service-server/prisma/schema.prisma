datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Query {
  createDate DateTime?
  createdAt  DateTime  @default(now())
  id         String    @id @default(cuid())
  queryText  String?
  results    Result[]
  updatedAt  DateTime  @updatedAt
  userId     String?
}

model Result {
  createdAt    DateTime  @default(now())
  data         Json?
  id           String    @id @default(cuid())
  query        Query?    @relation(fields: [queryId], references: [id])
  queryId      String?
  receivedDate DateTime?
  updatedAt    DateTime  @updatedAt
}
